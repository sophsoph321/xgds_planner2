<div class="row-fluid">
    <div id="map-buttons" data-toggle="buttons-radio" class="span6">
        <div class="btn-group">
            <button class="btn" id="btn-navigate">Navigate</button>
            <button class="btn" id="btn-reposition">Reposition</button>
            <button class="btn" id="btn-addStations">Add Stations</button>
            <!-- <button class="btn" id="btn-add-targets">Add Targets</button> -->
        </div>
    
    </div>
    <div id="tab-buttons" class="span6">
        <div class="btn-group">
            <button class="btn" id="btn-cut">Cut</button>
            <button class="btn" id="btn-copy">Copy</button>
            <button class="btn" id="btn-paste">Paste</button>
        </div>
        <button class="btn" id="btn-delete">Delete</button>

        <button class="btn" id="btn-edit" style="float:right">Stop Editing</button>

        <div class="btn-group" style="float: right">
            <button class="btn" id="btn-undo">Undo</button>
            <button class="btn" id="btn-redo">Redo</button>
            <button class="btn" id="btn-save">Save</button>
        </div>
    </div>
